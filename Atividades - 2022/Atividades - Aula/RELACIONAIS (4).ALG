Algoritmo "Relacionais (4)"
// Autor(a)    : Vinicius Andrade
// Data atual  : 26/6/2022
Var
   A, B, C: Inteiro

Inicio
      A <- 1
      B <- 2
      C <- 3
      
      Escreval(A > B)
      Escreval(B <= A)
      Escreval(B >= A)
      Escreval(B = 2)
      Escreval(B <> C)
      Escreval(C >= A^B)
      Escreval(1 = 5 % B)

Fimalgoritmo