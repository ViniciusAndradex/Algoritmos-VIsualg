Algoritmo "ex 90"
// Autor(a)    : Vinicius Andrade
// Data atual  : 4/7/2022
Var
   a, b  : Inteiro

   Procedimento Somador(a, b, : Inteiro)
   Inicio
         EscrevaL(a," + ", b," = ", a + b)
   FimProcedimento
Inicio
      Escreva("Digite um número: ")
      Leia(a)
      Escreva("Digite um número: ")
      Leia(b)
      LimpaTela
      Somador(a,b)
Fimalgoritmo