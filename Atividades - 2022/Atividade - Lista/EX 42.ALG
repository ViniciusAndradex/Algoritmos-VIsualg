Algoritmo "Ex 42"
// Autor(a)    : Vinicius Andrade
// Data atual  : 30/6/2022
Var
   user, cont : Inteiro
Inicio
      Escreva("Digite um número: ")
      Leia(user)
      cont <- 1
      Enquanto (cont <= user) faça
               Escreva(cont," ")
               cont <- cont + 1
      FimEnquanto
      EscrevaL("Acabou")
Fimalgoritmo