algoritmo "7"
// Função :
// Autor : Andrade
// Data : 14/02/2021
// Seção de Declarações 
var
   val,dobro: inteiro
   ter: real
inicio
      Escreva ("Digite um valor: ")
      Leia (val)
      dobro <- val * 2
      Escreval ("O dobro de",val," é",dobro)
      ter <- val / 3
      Escreva ("A terça parte de",val," é",ter)
fimalgoritmo
