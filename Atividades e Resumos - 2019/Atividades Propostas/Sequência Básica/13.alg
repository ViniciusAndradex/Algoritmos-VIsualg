algoritmo "13"
// Função :
// Autor : Andrade
// Data : 14/02/2021
// Seção de Declarações 
var
   sal,aum,aum1: real
inicio
      Escreva ("Qual o salário: ")
      Leia (sal)
      aum1 <- (sal * 15) / 100
      aum <- saL + aum1
      Escreva ("Você recevberá um aumento de R$",aum1," e seu salário novo é R$",aum)
fimalgoritmo
