algoritmo "6"
// Função :
// Autor :  Andrade
// Data : 14/02/2021
// Seção de Declarações 
var
   num,ant,suc: inteiro
inicio
      Escreva ("Digite um número: ")
      Leia (num)
      suc <- num + 1
      ant <- num -1
      Escreval ("O sucessor desse número é ",suc)
      Escreva ("O antecessor desse número é",ant)
fimalgoritmo
