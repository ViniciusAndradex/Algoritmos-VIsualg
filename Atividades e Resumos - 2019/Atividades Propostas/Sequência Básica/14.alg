algoritmo "semnome"
// Função :
// Autor :
// Data : 14/02/2021
// Seção de Declarações 
var
   km,dia: inteiro
   total: real
inicio
      Escreva ("Quantos Km foram rodados: ")
      Leia (km)
      Escreva ("Quantos dias foi alugado: ")
      Leia (dia)
      total <- (dia * 90) + (km * 0.20)
      Escreva ("Você terá que pagar R$",total)
fimalgoritmo
