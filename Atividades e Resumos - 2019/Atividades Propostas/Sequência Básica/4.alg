algoritmo "4"
// Função :
// Autor : Andrade
// Data : 14/02/2021
// Seção de Declarações 
var
   val,val1,soma: inteiro
inicio
      Escreva ("Digite um valor: ")
      Leia (val)
      Escreva ("Digite outro valor: ")
      Leia (val1)
      soma <- val + val1
      Escreva ("A soma entre os valores é",soma)
fimalgoritmo
